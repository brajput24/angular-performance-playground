{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "performance-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        {
          "glob": "**/*",
          "input": "./assets/",
          "output": "./assets/"
        },
        {
          "glob": "favicon.ico",
          "input": "./",
          "output": "./"
        },
        "worker.js",
        {
          "glob": "lodash.min.js",
          "input": "../node_modules/lodash",
          "output": "./"
        },
        {
          "glob": "d3-collection.min.js",
          "input": "../node_modules/d3-collection/build",
          "output": "./"
        },
        {
          "glob": "d3-dispatch.min.js",
          "input": "../node_modules/d3-dispatch/build",
          "output": "./"
        },
        {
          "glob": "d3-quadtree.min.js",
          "input": "../node_modules/d3-quadtree/build",
          "output": "./"
        },
        {
          "glob": "d3-timer.min.js",
          "input": "../node_modules/d3-timer/build",
          "output": "./"
        },
        {
          "glob": "d3-force.min.js",
          "input": "../node_modules/d3-force/build",
          "output": "./"
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {},
    "serve": {
      "port": 80
    }
  }
}